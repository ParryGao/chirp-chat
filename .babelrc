    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "actions": "./src/actions",
        "components": "./src/components",
        "constants": "./src/constants",
        "helpers": "./src/helpers",
        "modules": "./src/modules",
        "reducers": "./src/reducers",
        "resources": "./src/resources",
        "screens": "./src/screens",
        "store": "./src/store"
      }
    }]
  ]
}